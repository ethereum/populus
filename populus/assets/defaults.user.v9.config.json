{
  "version": "9",
  "chains": {
    "mainnet": {
      "chain": {
        "class": "populus.chain.geth.MainnetChain"
      },
      "web3": {
        "provider": {
          "class": "web3.providers.ipc.IPCProvider"
        }
      },
      "contracts": {
        "backends": {
          "JSONFile": {
            "class": "populus.contracts.backends.filesystem.JSONFileBackend",
            "priority": 10,
            "settings": {
              "file_path": "./registrar.json"
            }
          },
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      }
    },
    "ropsten": {
      "chain": {
        "class": "populus.chain.geth.TestnetChain"
      },
      "web3": {
        "provider": {
          "class": "web3.providers.ipc.IPCProvider"
        }
      },
      "contracts": {
        "backends": {
          "JSONFile": {
            "class": "populus.contracts.backends.filesystem.JSONFileBackend",
            "priority": 10,
            "settings": {
              "file_path": "./registrar.json"
            }
          },
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      }
    },
    "temp": {
      "chain": {
        "class": "populus.chain.geth.TemporaryGethChain"
      },
      "web3": {
        "provider": {
          "class": "web3.providers.ipc.IPCProvider"
        }
      },
      "contracts": {
        "backends": {
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      }
    },
    "tester": {
      "chain": {
        "class": "populus.chain.tester.TesterChain"
      },
      "web3": {
        "$ref": "web3.Tester"
      },
      "contracts": {
        "backends": {
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      }
    }
  },
  "contracts": {
    "backends": {
      "JSONFile": {
        "class": "populus.contracts.backends.filesystem.JSONFileBackend",
        "priority": 10,
        "settings": {
          "file_path": "./registrar.json"
        }
      },
      "ProjectContracts": {
        "class": "populus.contracts.backends.project.ProjectContractsBackend",
        "priority": 20
      },
      "TestContracts": {
        "class": "populus.contracts.backends.testing.TestContractsBackend",
        "priority": 40
      },
      "Memory": {
        "class": "populus.contracts.backends.memory.MemoryBackend",
        "priority": 50
      }
    }
  },
  "compilation": {
    "backends": {
      "SolcAutoBackend": {
        "class": "populus.compilation.backends.SolcAutoBackend",
        "settings": {
          "optimize": true,
          "output_values": ["abi", "bin", "bin-runtime", "metadata"]
        }
      },
      "SolcCombinedJSON": {
        "class": "populus.compilation.backends.SolcCombinedJSONBackend",
        "settings": {
           "optimizer": {
              "enabled": true,
              "runs": 500
            },
          "output_values": ["abi", "bin", "bin-runtime", "metadata"]
        }
      },
      "SolcStandardJSON": {
        "class": "populus.compilation.backends.SolcStandardJSONBackend",
        "settings": {
          "stdin": {
            "optimizer": {
              "enabled": true,
              "runs": 500
            },
            "outputSelection": {
              "*": {"*": ["abi", "metadata", "evm"]}
            }
          }
        }
      }
    },
    "backend": {
      "$ref": "compilation.backends.SolcAutoBackend"
    },
    "contracts_source_dirs": ["./contracts"],
    "import_remappings": []
  },
  "web3": {
    "GethIPC": {
      "provider": {
        "class": "web3.providers.ipc.IPCProvider"
      }
    },
    "InfuraMainnet": {
      "eth": {
        "default_account": "0x0000000000000000000000000000000000000001"
      },
      "provider": {
        "class": "web3.providers.HTTPProvider",
        "settings": {
          "endpoint_uri": "https://mainnet.infura.io"
        }
      }
    },
    "InfuraRopsten": {
      "eth": {
        "default_account": "0x0000000000000000000000000000000000000001"
      },
      "provider": {
        "class": "web3.providers.HTTPProvider",
        "settings": {
          "endpoint_uri": "https://ropsten.infura.io"
        }
      }
    },
    "Tester": {
      "provider": {
        "class": "web3.providers.tester.EthereumTesterProvider"
      }
    }
  }
}
